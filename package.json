{
  "name": "@aptero/axolotis-core-plugins",
  "version": "1.0.0",
  "description": "Axolotis - Core Plugins",
  "main": "build/index.js",
  "exports": {
    ".": "build/index.js",
    "./*": "./build/@aptero/axolotis-core-plugins/*"
  },
  "types": "build/types/index.d.ts",
  "scripts": {
    "pre-build": "node ./generator/generate-webpack-module.js",
    "only-build": "webpack && tsc && ttsc --emitDeclarationOnly",
    "start": "webpack serve --config webpack.config.demo.js",
    "build": "cross-env npm run pre-build && webpack && tsc",
    "build:demo": "webpack --config webpack.config.demo.js",
    "dev-watch": "tsc-watch --onSuccess \"yarn run only-build\"",
    "test": "jest --silent",
    "coverage": "npm run test -- --coverage",
    "prepare": "npm run build",
    "trypublish": "npm publish || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ApteroSAS/axolotis-player"
  },
  "author": "Aptero <admin@aptero.co> (https://github.com/ApteroSAS/axolotis-player)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ApteroSAS/axolotis-player/issues"
  },
  "homepage": "https://aptero.co",
  "keywords": [
    "library",
    "starter",
    "es6"
  ],
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@types/jest": "^27.4.1",
    "@types/three": "^0.139.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.4",
    "babel-preset-minify": "^0.5.1",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.13.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.6.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.6.2",
    "prettier-webpack-plugin": "^1.2.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "tsc-watch": "^5.0.3",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "4.8.1"
  },
  "dependencies": {
    "@aptero/axolotis-player": "git+https://github.com/ApteroSAS/axolotis-player.git#bdd5283655d7c75c2be33f1096efec835253328c",
    "eventemitter3": "^4.0.7",
    "phoenix": "^1.6.7",
    "three": "^0.139.2",
    "three-pathfinding": "^1.1.0",
    "ts-jest": "^27.1.4",
    "tsutils": "^3.21.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleNameMapper": {
      "@root(.*)$": "<rootDir>/src/$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/testMock.js",
      "\\.(css|less)$": "<rootDir>/scripts/testMock.js"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
