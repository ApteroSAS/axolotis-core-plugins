{
  "name": "@aptero/axolotis-core-plugins",
  "version": "1.0.0",
  "description": "Axolotis - Core Plugins",
  "main": "build/index.js",
  "exports": {
    ".": "build/index.js",
    "./*": "./build/@aptero/axolotis-core-plugins/*"
  },
  "types": "build/types/index.d.ts",
  "scripts": {
    "build:pre": "node ./generator/generate-webpack-module.js",
    "build:post": "webpack && tsc && ttsc --emitDeclarationOnly",
    "build": "cross-env yarn run build:pre && yarn run build:post",
    "build:demo": "webpack --config webpack.config.demo.js",
    "start": "yarn run dev",
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.config.demo.js",
    "dev:watch": "tsc-watch --onSuccess \"yarn run only-build\"",

    "test": "npx playwright test",
    "test:debug": "npx playwright test --headed",
    "coverage:serve": "cross-env NODE_ENV=development nyc yarn run start",
    "coverage": "cross-env NODE_ENV=development nyc npx playwright test",
    "trypublish": "yarn publish || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ApteroSAS/axolotis-player"
  },
  "author": "Aptero <admin@aptero.co> (https://github.com/ApteroSAS/axolotis-player)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ApteroSAS/axolotis-player/issues"
  },
  "homepage": "https://aptero.co",
  "keywords": [
    "library",
    "starter",
    "es6"
  ],
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@playwright/test": "^1.21.0",
    "@types/three": "^0.139.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.4",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-preset-minify": "^0.5.1",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.13.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "mini-css-extract-plugin": "^2.6.0",
    "npm": "^8.7.0",
    "nyc": "^15.1.0",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "playwright": "^1.21.0",
    "playwright-test-coverage": "^1.0.4",
    "prettier": "^2.6.2",
    "prettier-webpack-plugin": "^1.2.0",
    "source-map-support": "^0.5.21",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsc-watch": "^5.0.3",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "4.8.1"
  },
  "dependencies": {
    "@aptero/axolotis-player": "git+https://github.com/ApteroSAS/axolotis-player.git#bdd5283655d7c75c2be33f1096efec835253328c",
    "eventemitter3": "^4.0.7",
    "phoenix": "^1.6.7",
    "three": "^0.139.2",
    "three-pathfinding": "^1.1.0",
    "tsutils": "^3.21.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
